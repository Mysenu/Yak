QSplitter {
    background: #2b323f;
}

QSplitter::handle {
    background: #3a4e6b;
    margin-left: 2px;
    margin-right: 2px;
}

QListView {
    background: #2b323f;
    border: 0px;
    color: rgb(220, 220, 220);
}

QListView::item:selected {
    background: #242a35;
}

QLineEdit#ExpressionField {
    background: transparent;
    border-width: 0px;
    border-style: solid;
    border-color: #76c2af;
    color: rgb(220, 220, 220);
}

QListView#History QLineEdit {
    background: #242a35;
    color: rgb(220, 220, 220);
    border: 0px;
}

QMessageBox {
    background: #2b323f;
}

QMessageBox QPushButton {
    background: #3a4e6b;
    color: rgb(220, 220, 220);
}

QMessageBox QLabel {
    color: rgb(220, 220, 220);
}

QMessageBox QCheckBox {
    color: rgb(220, 220, 220);
}

Button {
    background: #3a4e6b;
    border-width: 0px;
    border-style: solid;
    color: rgb(220, 220, 220);
}

Button:pressed {
    background: #4e6991;
}

Button:hover {
    border-width: 1px;
    border-style: groove;
    border-color: #4e6991;
}

Button#Result {
    background: #e0995e;
}

Button#Result:pressed {
    background: #ffae6b;
}

Button#Clear {
    background: #e05e5e;
}

Button#Clear:pressed {
    background: #ff6b6b;
}

QMenu {
    background: #3a4e6b;
    color: rgb(220, 220, 220);
    border-width: 1px;
    border-style: solid;
    border-color: #2b323f;
}

QMenu::item:enabled:selected {
    background: #2b323f;
}

QMenu::item:disabled {
    color: rgb(136, 136, 136);
}

QMenu::item:selected:pressed {
    background: #323b4a;
}
